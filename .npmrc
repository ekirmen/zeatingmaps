# Optimizaciones de npm para Vercel
prefer-offline=true
audit=false
fund=false
progress=false
loglevel=error
silent=true

# Cache optimizado
cache-min=3600
cache-max=infinity

# Configuración de red
fetch-retries=3
fetch-retry-mintimeout=5000
fetch-retry-maxtimeout=60000

# Optimizaciones específicas para Vercel
registry=https://registry.npmjs.org/ 