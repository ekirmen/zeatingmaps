REACT_APP_SUPABASE_URL=<your-supabase-url>
REACT_APP_SUPABASE_ANON_KEY=<your-anon-key>
# Optional: required for admin features like blocking seats
REACT_APP_SITE_URL=http://localhost:3000
# You may also use REACT_SUPABASE_SERVICE_ROLE_KEY for compatibility
REACT_APP_SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>
# Optional bucket names. Defaults are 'eventos' and 'logos'
REACT_APP_EVENT_BUCKET=eventos
REACT_APP_LOGO_BUCKET=logos
# Optional subfolders inside each bucket (no leading or trailing slashes)
REACT_APP_EVENT_FOLDER=
REACT_APP_LOGO_FOLDER=
