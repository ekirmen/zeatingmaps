{
  "name": "ekirmen",
  "version": "0.1.8",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "homepage": "/store",
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "craco build",
    "vercel-build": "npm run build:compact",
    "build:compact": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true craco build",
    "preview": "serve -s build",
    "clean": "rm -rf node_modules package-lock.json build",
    "reinstall": "npm run clean && npm install --legacy-peer-deps",
    "force-build": "npm run reinstall && npm run build:compact",
    "analyze": "cross-env ANALYZE=true npm run build:compact",
    "analyze:clean": "rmdir /s /q build 2>nul || true && cross-env ANALYZE=true GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true craco build",
    "analyze:view": "start build/bundle-report.html"
  },
  "dependencies": {
    "@ant-design/icons": "5.3.0",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@rc-component/qrcode": "1.0.0",
    "@reduxjs/toolkit": "2.11.1",
    "@supabase/supabase-js": "2.50.0",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.0.0",
    "antd": "5.26.7",
    "axios": "1.9.0",
    "crypto-js": "4.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "11.0.0",
    "i18next": "23.8.1",
    "konva": "9.3.10",
    "lucide-react": "0.513.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.2",
    "react-i18next": "13.2.2",
    "react-icons": "5.5.0",
