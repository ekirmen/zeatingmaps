    "build:no-lint": "npx cross-env DISABLE_ESLINT_PLUGIN=true craco build",
    "build:diag": "npx cross-env ANALYZE=true DISABLE_ESLINT_PLUGIN=true craco build",
    "preview": "serve -s build",
    "eject": "react-scripts eject",
    "vercel-build": "react-scripts build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:serve": "npm run build && npx serve -s build",
    "analyze:dead-code": "node scripts/remove-dead-code.js",
    "images:convert": "node scripts/convert-images.js",
    "test:endpoints": "node scripts/test-endpoints.js",
    "test:quick": "node scripts/quick-test.js",
