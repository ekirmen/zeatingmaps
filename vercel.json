{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/store/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/saas/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/backoffice/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/payment-success/:path*",
      "destination": "/index.html"
    }
  ]
}